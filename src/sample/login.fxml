<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LoginController">
   <children>
      <TextField fx:id="TextUserName" layoutX="183.0" layoutY="126.0" prefHeight="25.0" prefWidth="235.0" promptText="User name">
         <font>
            <Font size="16.0" />
         </font></TextField>
      <Button fx:id="loginBtn" layoutX="264.0" layoutY="270.0" mnemonicParsing="false" onAction="#login" prefHeight="39.0" prefWidth="73.0" text="Login">
         <font>
            <Font size="16.0" />
         </font></Button>
      <Label fx:id="isConnected" layoutX="88.0" layoutY="28.0" prefHeight="39.0" prefWidth="115.0" text="REsultat" />
      <PasswordField fx:id="TextPassword" layoutX="183.0" layoutY="200.0" prefHeight="37.0" prefWidth="235.0" promptText="Password...">
         <font>
            <Font size="16.0" />
         </font>
      </PasswordField>
   </children>
</AnchorPane>
